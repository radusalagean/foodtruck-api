{
  "name": "foodtruck-api",
  "version": "0.0.1",
  "description": "Node.js API from https://www.udemy.com/api-development",
  "main": "dist",
  "scripts": {
    "dev": "SET NODE_ENV=development & nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "SET NODE_ENV=production & pm2 start dist",
    "prestart": "npm run -s build",
    "lint": "eslint src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/radusalagean/restaurant-api.git"
  },
  "author": "Radu Salagean <radusalagean@hotmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/radusalagean/restaurant-api/issues"
  },
  "homepage": "https://github.com/radusalagean/restaurant-api#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "mongoose": "^5.5.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^5.16.0",
    "nodemon": "^1.18.11"
  }
}
